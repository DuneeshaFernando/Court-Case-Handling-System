	<parameter name="decisionparam" isForPrompting="true" class="java.lang.String"/>
	<parameter name="enddecisionparam" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT
     decision.`caseNumber` AS decision_caseNumber,
     decision.`judgeNumber` AS decision_judgeNumber,
     decision.`result` AS decision_result,
     decision.`resultDescri` AS decision_resultDescri,
     casedata.`casenum` AS casedata_casenum,
     casedata.`hearingDate` AS casedata_hearingDate,
     judge.`name` AS judge_name
FROM
     `casedata` casedata INNER JOIN `decision` decision ON casedata.`casenum` = decision.`caseNumber`
     INNER JOIN `judge` judge ON decision.`judgeNumber` = judge.`judgeNumber`
WHERE
     casedata.`hearingDate` >= $P{decisionparam} AND casedata.`hearingDate` <= $P{enddecisionparam}]]></queryString>

