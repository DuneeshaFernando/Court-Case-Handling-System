	<queryString><![CDATA[SELECT
     receipt.`receiptNumber` AS receipt_receiptNumber,
     receipt.`amount` AS receipt_amount,
     receipt.`date` AS receipt_date,
     receipt.`description` AS receipt_description,
     receipt.`caseNumber` AS receipt_caseNumber,
     plaintiff.`name` AS plaintiff_name
FROM
     `casedata` casedata INNER JOIN `receipt` receipt ON casedata.`casenum` = receipt.`caseNumber`
     INNER JOIN `plaintiffcase` plaintiffcase ON casedata.`casenum` = plaintiffcase.`caseNumber`
     INNER JOIN `plaintiff` plaintiff ON plaintiffcase.`plaintiffID` = plaintiff.`plaintiffID`
WHERE
     receipt.`receiptNumber` = $P{firstparam}]]></queryString>

