	<queryString><![CDATA[SELECT
     decision.`caseNumber` AS decision_caseNumber,
     casedata.`value` AS casedata_value,
     casedata.`nature` AS casedata_nature,
     casedata.`type` AS casedata_type,
     casedata.`procedure` AS casedata_procedure,
     decision.`result` AS decision_result,
     decision.`resultDescri` AS decision_resultDescri,
     plaintiff.`name` AS plaintiff_name,
     defendant.`name` AS defendant_name,
     defendant.`address` AS defendant_address,
     plaintiff.`address` AS plaintiff_address
FROM
     `casedata` casedata INNER JOIN `decision` decision ON casedata.`casenum` = decision.`caseNumber`
     INNER JOIN `plaintiffcase` plaintiffcase ON casedata.`casenum` = plaintiffcase.`caseNumber`
     INNER JOIN `defcase` defcase ON casedata.`casenum` = defcase.`caseNumber`
     INNER JOIN `defendant` defendant ON defcase.`defID` = defendant.`defID`
     INNER JOIN `plaintiff` plaintiff ON plaintiffcase.`plaintiffID` = plaintiff.`plaintiffID`
WHERE
     decision.`result` = "Exparty" AND decision.`caseNumber`=$P{firstparam}]]></queryString>

